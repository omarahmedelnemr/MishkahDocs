            {
                "paramName": "newTime",
                "paramType": "string",
                "paramDescription": "The new Day time for the track meetings"
            }
        ],
        "requestExample": {
            "trackID": 324,
            "newDay": "Tuesday",
            "newTime":"05:00PM"
